{
	"name": "MultiTitle",
	"author": [
		"Tbodt"
	],
	"url": "https://www.mediawiki.org/wiki/Special:MyLanguage/Extension:MultiTitle",
	"descriptionmsg": "multitle-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"Hooks": {
		"ArticleViewRedirect": "main",
		"ArticleViewFooter": "main",
		"GetDoubleUnderscoreIDs": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\MultiTitle\\Hooks",
			"services": [
				"PageProps"
			]
		}
	},
	"ExtensionMessagesFiles": {
		"MultiTitleMagic": "MultiTitle.i18n.magic.php"
	},
	"MessagesDirs": {
		"MultiTitle": [
			"i18n"
		]
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\MultiTitle\\": "includes/"
	},
	"manifest_version": 2
}
